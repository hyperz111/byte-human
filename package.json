{
	"name": "byte-human",
	"version": "1.0.1",
	"description": "Convert byte size to human-readable size",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"scripts": {
		"test": "pnpm run build && uvu test",
		"format": "prettier -w **/*.{js*,ts,md} !dist",
		"build": "tsc",
		"prepublishOnly": "pnpm run build"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"byte",
		"bytes",
		"convert",
		"conversion",
		"human-readable",
		"kilobyte",
		"megabyte",
		"gigabyte",
		"terabyte"
	],
	"author": "Hyper-Z11 (https://github.com/hyperz111)",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hyperz111/byte-human.git"
	},
	"bugs": {
		"url": "https://github.com/hyperz111/byte-human/issues"
	},
	"homepage": "https://github.com/hyperz111/byte-human#readme",
	"packageManager": "pnpm@10.13.1",
	"devDependencies": {
		"@hyperz111/prettier-config": "^1.0.0",
		"@hyperz111/tsconfig": "^0.1.0",
		"prettier": "^3.6.2",
		"typescript": "^5.9.2",
		"uvu": "^0.5.6"
	}
}
